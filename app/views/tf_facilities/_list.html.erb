<table class="data">
	<thead>
		<tr>
			<th width="1%"><%= check_box_tag 'list_select_all', 1, false %><br>ID</th>
			<th width="1%">Created<br>Risk</th>
			<th width="1%">Issued<br>Expires</th>
			<th width="49%">Operator<br>Permit</th>
			<th width="49%">Food&nbsp;Stand<br>Event</th>
		</tr>
	</thead>
	<tbody>
		<% reset_cycle %>
		<% objs.each { |o| %>
			<tr class="<%= cycle 'odd', 'even' %>">
				<td class="nobr"><%= check_box_tag 'list[]', o.id, false, class: 'list_select', id: nil %><br><%= o.id %></td>
				<td class="nobr"><%= o.created_at.d %>&nbsp;<br><%= lbl 'risk', o.risk_class %></td>
				<td class="nobr"><%= o.issue_date.d %>&nbsp;<br><%= o.expire_date.d %></td>
				<td><%= link_to o.operator_name, controller: :tf_facilities, action: :view, id: o.id %>&nbsp;<br><%= o.temp_permit_number %></td>
				<td><%= o.food_stand %><br><%= o.event_name %></td>
			</tr>
		<% } %>
	</tbody>
</table>