<table class="data">
	<thead>
		<tr>
			<th width="1%" class="nobr"><%= list_check_all %>&nbsp;ID</th>
			<th width="1%">Division</th>
			<th width="1%" class="tr">Count</th>
			<th width="1%" class="tr">Tot</th>
			<th width="1%">User</th>
			<th width="1%">Created</th>
			<th>Default Item</th>
		</tr>
	</thead>
	<tbody>
		<% reset_cycle %>
		<% objs.each { |o| %>
			<tr class="<%= cycle 'odd', 'even' %>">
				<td class="nobr"><%= list_check_id o.id %>&nbsp;<%= o.id %></td>
				<td class="nobr"><%= lbl 'qbdiv', o.division %></td>				
				<td class="nobr tr"><%= n0 o.qb_transaction_details_count %></td>
				<td class="nobr tr"><%= n2 o.total %></td>
				<td class="nobr"><% if o.user %><%= o.user.username %><% else %><span class="dim">System</span><% end %></td>
				<td class="nobr"><%= link_to blank_tag(o.created_at.d), controller: :qb_late_fees, action: :view, id: o.id %></td>
				<td><%= o.item_name %></td>
			</tr>
		<% } %>
	</tbody>
</table>